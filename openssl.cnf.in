HOME = .

# Use this in order to automatically load providers.
openssl_conf = openssl_init

config_diagnostics = 1

[openssl_init]
providers = provider_sect
alg_section = evp_properties

[provider_sect]
default = default_sect
base = base_sect
hbkzpc = hbkzpc_sect

[evp_properties]

[base_sect]
activate = 1

[default_sect]
activate = 1

[hbkzpc_sect]
module = @ZPCPROVIDER_MODULE@
identity = hbkzpc
activate = 1
